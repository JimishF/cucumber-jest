{
  "name": "cucumber-jest-example",
  "version": "0.0.1",
  "description": "a basic example of using cucumber-jest",
  "scripts": {
    "test": "ENV=dev jest --verbose --detectLeaks --detectOpenHandles --logHeapUsage",
    "test:debug": "jest --runInBand --logHeapUsage"
  },
  "author": "dayne mentier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mentierd/cucumber-jest/issues"
  },
  "homepage": "https://github.com/mentierd/cucumber-jest#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "axios": "^0.21.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-transform-typescript": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@types/cucumber": "^6.0.1",
    "@types/jest": "^25.2.1",
    "@types/qs": "^6.9.5",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "babel-plugin-lodash": "^3.3.4",
    "cucumber-jest": "file:../cucumber-jest-0.0.20.tgz",
    "deep-clean": "^1.0.2",
    "jest-date-mock": "^1.0.8",
    "msw": "^0.24.1",
    "qs": "^6.9.4",
    "typescript": "^4.1.2",
    "weak-napi": "^2.0.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "feature",
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "restoreMocks": true,
    "setupFiles": [
      "<rootDir>/node_modules/cucumber-jest/dist/init.js",
      "<rootDir>/node_modules/cucumber-jest/dist/window.js",
      "jest-date-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/world.ts",
      "<rootDir>/test/hooks.tsx",
      "<rootDir>/test/steps.ts"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest",
      "^.+\\.feature$": "cucumber-jest"
    },
    "testMatch": [
      "<rootDir>/test/features/*.feature"
    ],
    "testTimeout": 60000
  }
}
